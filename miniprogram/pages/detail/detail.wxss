.top_contain {
      display: flex;
      flex-direction: column;
      padding: 24rpx;
      box-sizing: border-box;
}

.top_img {
      width: 100%;
      display: flex;
      justify-content: center;
}

.top_img image {
      width: 600rpx;
      height: 600rpx;
}

.left {
      width: 100%;
      height: 50rpx;
      background: #eee;
      text-align: center;
      color: #616161;
      /* padding: 30px 15px; */
}

.title {
      padding-top: 30rpx;
      font-size: 32rpx;
      font-weight: 600;
      letter-spacing: 2rpx;
}

.price_box {
      margin-top: 20rpx;
      display: flex;
      align-items: flex-end;
}

.now_price {
      font-size: 34rpx;
      color: rgb(255, 1, 1);
}

.pre_price {
      padding-left: 30rpx;
      font-size: 28rpx;
      color: rgb(172, 171, 171);
      text-decoration: line-through;
}

.blank {
      width: 100%;
      height: 24rpx;
      background: #eee;
}

.center_contain {
      width: 100%;
      height: 80rpx;
      display: flex;
      box-sizing: border-box;
      padding: 0 100rpx;
      justify-content: space-around;
}

.c_title {
      font-size: 30rpx;
      width: 140rpx;
      height: 100%;
      display: flex;
      letter-spacing: 2rpx;
      align-items: center;
      justify-content: center;
}

.title_on {
      font-size: 32rpx;
      font-weight: 600;
      color: #000;
      border-bottom: 4rpx solid #4db7ff;
}

.user_box {
      width: 100%;
      height: 148rpx;
      display: flex;
      padding: 24rpx;
      align-items: flex-start;
      /* justify-content: space-between; */
      box-sizing: border-box;
}

.user_box image {
      width: 100rpx;
      height: 100rpx;
      border-radius: 50%;
}

.des_box {
      height: 100%;
      width: 450rpx;
      display: flex;
      flex-direction: column;
      align-items: flex-start;
      justify-content: space-around;
      /* padding: 10rpx 0; */
      padding: 10rpx 0 0 30rpx;

}

.user_name {
      font-size: 32rpx;
      letter-spacing: 2rpx;
}

.local_box {
      display: flex;
      align-items: center;
}

.local_box image {
      width: 30rpx;
      height: 30rpx;
}

.local_box view {
      padding-left: 5rpx;
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: rgb(150, 150, 150);
}

.good_Info{
      margin-top: 50rpx;
     margin-left: 100rpx;
     margin-bottom: 80rpx;
     margin-right: 100rpx; 
     border-color: #cbcbcc;
     border-style: solid;
     display: flex;
     flex-direction:column;
     border-radius: 30rpx;
   }

.time_box {
      width: 100%;
      padding: 24rpx 24rpx 0 24rpx;
      display: flex;
      box-sizing: border-box;
      justify-content: space-between;
}

.kind {
      font-size: 28rpx;
      letter-spacing: 2rpx;
}

.time {
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: #8c9aa8;
}

.copy_box {
      display: flex;
      align-items: center;
      padding: 0 0 0 20rpx;
}

.copy_box image {
      width: 40rpx;
      height: 40rpx;
}

.copy_box view {
      padding-left: 5rpx;
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: rgb(150, 150, 150);
}

.deliver_box {
      width: 100%;
      padding: 24rpx 24rpx 0 24rpx;
      display: flex;
      box-sizing: border-box;
      /* justify-content: space-between; */
}

.deliver_first {
      display: flex;
      align-items: flex-start;
}

.deliver_title {
      font-size: 28rpx;
      letter-spacing: 2rpx;
      color: #a5a5a5;
}

.wxlist {
      font-size: 28rpx;
      display: flex;
      flex-direction: column;
      align-items: flex-start;
      justify-content: space-around;
}

.downicon{
      width: 30rpx;
      height: 30rpx;
}

.deliver_kind {
      font-size: 28rpx;
      letter-spacing: 2rpx;
      /* color: #a5a5a5; */
}

.deliver_place {
      font-size: 28rpx;
      letter-spacing: 2rpx;
}

.checkPeop{
      width: 140rpx;
      height: 50rpx;
      display: flex;
      justify-content: center;
      align-items: center;
      color: #616161;
      font-size: 30rpx;
      letter-spacing: 2rpx;
      padding: 0 0 0 10rpx;
      /* box-shadow: 0 0 20rpx #eee; */
      border: none;
    }

.submitGoodinfo {
      /* margin-top: 24rpx; */
      width: calc(100% - 300rpx);
      /* width: 60rpx; */
      height: 60rpx;
      background: #f1f3f8;
      border-radius: 10rpx;
      text-align: left;
      padding: 0 36rpx;
      box-sizing: border-box;
      font-size: 32rpx;
      display: flex;
      flex-direction:row;
      align-items: center;
      justify-content: space-between;
      color: #8c9aa8;
    }

.submitGoodButton {
      /* margin-top: 50rpx;
      width: 100%; */
      width: 140rpx;
      height: 50rpx;
      display: flex;
      justify-content: center;
      align-items: center;
      /* background: linear-gradient(#4db7ff, #8dc6ec); */
      border-radius: 60rpx;
      color: #616161;
      font-size: 30rpx;
      letter-spacing: 4rpx;
      /* box-shadow: 0 0 20rpx #eee; */
      border: 1rpx solid #616161;
    }
    .submitButton {
      /* margin-top: 50rpx; */
      width: 150rpx;
      height: 60rpx;
      display: flex;
      justify-content: center;
      align-items: center;
      background: linear-gradient(#4db7ff, #8dc6ec);
      border-radius: 60rpx;
      color: #fff;
      font-size: 30rpx;
      letter-spacing: 4rpx;
      box-shadow: 0 0 20rpx #eee;
      border: none;
    }

/*底部导航*/

.tabbar {
      width: 100%;
      height: 120rpx;
      background: #fff;
      opacity: 0.9;
      position: fixed;
      left: 0rpx;
      bottom: 0rpx;
      display: flex;
      padding: 10rpx;
      /* border-top: 1rpx solid #eee; */
      align-items: center;
      justify-content: center;
      box-sizing: border-box;
      z-index: 8;
}

.t_button {
      width: 100%;
      height: 100%;
      position: absolute;
      left: 0rpx;
      top: 0rpx;
      opacity: 0.01;
      z-index: 9;
}

.buy_box {
      width: 40%;
      height: 100%;
      display: flex;
      justify-content: center;
      align-items: center;
      box-sizing: border-box;
}

.buy {
      width: 90%;
      height: 70rpx;
      background: #4db7ff;
      color: rgb(255, 255, 255);
      letter-spacing: 4rpx;
      font-size: 28rpx;
      border-radius: 35rpx;
      display: flex;
      justify-content: center;
      align-items: center;
}
